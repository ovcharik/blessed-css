{
    "version": "2.0.0",
    "tasks": [
      {
        "identifier": "npm:build",
        "type": "npm",
        "script": "build",
        "problemMatcher": [
          "$tsc",
          {
            "base": "$tslint5",
            "fileLocation": "relative"
          }
        ],
        "group": "build"
      },
      {
        "identifier": "npm:compile",
        "type": "npm",
        "script": "compile",
        "problemMatcher": [
          "$tsc"
        ],
        "group": "build"
      },
      {
        "identifier": "shell:compile:silent",
        "label": "shell: compile:silent",
        "type": "shell",
        "command": "${workspaceFolder}/node_modules/.bin/tsc --build tsconfig.json",
        "problemMatcher": [
          "$tsc"
        ],
        "presentation": {
          "echo": false,
          "reveal": "silent"
        },
        "group": "build"
      },
      {
        "identifier": "npm:watch",
        "type": "npm",
        "script": "watch",
        "isBackground": true,
        "problemMatcher": [
          "$tsc-watch"
        ],
        "presentation": {
          "echo": false,
          "reveal": "never",
          "panel": "dedicated"
        },
        "group": "build"
      },
      {
        "identifier": "npm:clean",
        "type": "npm",
        "script": "clean",
        "problemMatcher": [],
        "group": "none"
      },
      {
        "identifier": "npm:lint",
        "type": "npm",
        "script": "lint",
        "problemMatcher": [
          {
            "base": "$tslint5",
            "fileLocation": "relative"
          }
        ],
        "group": "test"
      },
      {
        "identifier": "npm:test",
        "type": "npm",
        "script": "test",
        "problemMatcher": []
      }
    ]
  }
