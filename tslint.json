{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-plugin-prettier", "tslint-config-prettier"],
  "rules": {
    "prettier": true,
    "no-console": {
      "severity": "warning"
    },
    "no-empty": {
      "severity": "warning"
    },
    "no-empty-interface": {
      "severity": "warning"
    },
    "quotemark": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "max-line-length": {
      "options": {
        "limit": 100
      },
      "severity": "warning"
    }
  }
}
